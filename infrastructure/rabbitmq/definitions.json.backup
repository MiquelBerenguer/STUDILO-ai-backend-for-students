{
  "rabbit_version": "3.12",
  "rabbitmq_version": "3.12",
  "vhosts": [
    {
      "name": "tutor_ia"
    }
  ],
  "permissions": [
    {
      "user": "tutor_user",
      "vhost": "tutor_ia",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "tutor.processing",
      "vhost": "tutor_ia",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "tutor.direct",
      "vhost": "tutor_ia",
      "type": "direct",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "tutor.dlx",
      "vhost": "tutor_ia",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    }
  ],
  "queues": [
    {
      "name": "pdf.process",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "tutor.dlx",
        "x-dead-letter-routing-key": "pdf.failed",
        "x-max-length": 10000,
        "x-max-priority": 5
      }
    },
    {
      "name": "pdf.process.priority",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-dead-letter-exchange": "tutor.dlx",
        "x-dead-letter-routing-key": "pdf.failed.priority",
        "x-max-length": 1000,
        "x-max-priority": 10
      }
    },
    {
      "name": "ai.embeddings",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-dead-letter-exchange": "tutor.dlx",
        "x-dead-letter-routing-key": "ai.embeddings.failed",
        "x-max-length": 50000
      }
    },
    {
      "name": "ai.analysis",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-dead-letter-exchange": "tutor.dlx",
        "x-dead-letter-routing-key": "ai.analysis.failed",
        "x-max-length": 5000
      }
    },
    {
      "name": "notifications",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-max-length": 100000
      }
    },
    {
      "name": "dlx.failed",
      "vhost": "tutor_ia",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 50000,
        "x-queue-mode": "lazy"
      }
    }
  ],
  "bindings": [
    {
      "source": "tutor.processing",
      "vhost": "tutor_ia",
      "destination": "pdf.process",
      "destination_type": "queue",
      "routing_key": "pdf.process",
      "arguments": {}
    },
    {
      "source": "tutor.processing",
      "vhost": "tutor_ia",
      "destination": "pdf.process.priority",
      "destination_type": "queue",
      "routing_key": "pdf.process.priority",
      "arguments": {}
    },
    {
      "source": "tutor.direct",
      "vhost": "tutor_ia",
      "destination": "ai.embeddings",
      "destination_type": "queue",
      "routing_key": "embeddings",
      "arguments": {}
    },
    {
      "source": "tutor.direct",
      "vhost": "tutor_ia",
      "destination": "ai.analysis",
      "destination_type": "queue",
      "routing_key": "analysis",
      "arguments": {}
    },
    {
      "source": "tutor.direct",
      "vhost": "tutor_ia",
      "destination": "notifications",
      "destination_type": "queue",
      "routing_key": "notify",
      "arguments": {}
    },
    {
      "source": "tutor.dlx",
      "vhost": "tutor_ia",
      "destination": "dlx.failed",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
    }
  ]
}